S_0x55d49e07a0d0 .scope module, "counter_up_down_tb" "counter_up_down_tb" 2 1;
 .timescale 0 0;
P_0x55d49e07a250 .param/l "clock_period" 0 2 10, +C4<00000000000000000000000000001010>;
