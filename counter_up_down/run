    .scope S_0x55d49e0b29f0;
T_1 ;
    %wait E_0x55d49e0b3d70;
    %load/vec4 v0x55d49e0c7950_0;
    %assign/vec4 v0x55d49e0c7c10_0, 0;
    %load/vec4 v0x55d49e0c7b00_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55d49e0c7950_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55d49e0c7a40_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x55d49e0c7950_0;
    %assign/vec4 v0x55d49e0c7950_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x55d49e0b2c70_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.4, 4;
    %load/vec4 v0x55d49e0c7950_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55d49e0c7950_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v0x55d49e0c7950_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x55d49e0c7950_0, 0;
T_1.5 ;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55d49e07a0d0;
T_2 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d49e0c8020_0, 0, 32;
    %end;
    .thread T_2;
    .scope S_0x55d49e07a0d0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d49e0c7e50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d49e0c80c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d49e0c7f20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d49e0c7d90_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d49e0c8020_0, 0, 32;
T_3.0 ;
    %load/vec4 v0x55d49e0c8020_0;
    %cmpi/s 13, 0, 32;
    %jmp/0xz T_3.1, 5;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d49e0c80c0_0, 0, 1;
    %load/vec4 v0x55d49e0c8020_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x55d49e0c8020_0, 0, 32;
    %jmp T_3.0;
T_3.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d49e0c7d90_0, 0, 1;
