    %vpi_call 2 23 "$display", "Now start counting down" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d49e0c8020_0, 0, 32;
T_3.2 ;
    %load/vec4 v0x55d49e0c8020_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz T_3.3, 5;
    %delay 5, 0;
