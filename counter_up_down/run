v0x55d49e0c7c10_0 .var "sum", 3 0;
E_0x55d49e0b3d70 .event posedge, v0x55d49e0c7b00_0, v0x55d49e0c7890_0;
    .scope S_0x55d49e0b29f0;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55d49e0c7950_0, 0, 4;
    %end;
