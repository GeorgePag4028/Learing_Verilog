#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55f4cd734100 .scope module, "dec_3to8_tb" "dec_3to8_tb" 2 1;
 .timescale 0 0;
v0x55f4cd77f0a0_0 .var "d_in", 2 0;
v0x55f4cd77f190_0 .net "d_out", 7 0, v0x55f4cd77eea0_0;  1 drivers
v0x55f4cd77f260_0 .var "e", 0 0;
v0x55f4cd77f360_0 .var/i "i", 31 0;
S_0x55f4cd734280 .scope module, "dec" "dec_3to8" 2 8, 3 1 0, S_0x55f4cd734100;
 .timescale 0 0;
    .port_info 0 /INPUT 3 "d_in"
    .port_info 1 /INPUT 1 "e"
    .port_info 2 /OUTPUT 8 "d_out"
v0x55f4cd76b4a0_0 .net "d_in", 2 0, v0x55f4cd77f0a0_0;  1 drivers
v0x55f4cd77eea0_0 .var "d_out", 7 0;
v0x55f4cd77ef80_0 .net "e", 0 0, v0x55f4cd77f260_0;  1 drivers
E_0x55f4cd7693f0 .event edge, v0x55f4cd76b4a0_0;
    .scope S_0x55f4cd734280;
T_0 ;
    %wait E_0x55f4cd7693f0;
    %load/vec4 v0x55f4cd77ef80_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x55f4cd76b4a0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_0.6, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 3;
    %cmp/u;
    %jmp/1 T_0.7, 6;
    %dup/vec4;
    %pushi/vec4 6, 0, 3;
    %cmp/u;
    %jmp/1 T_0.8, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 3;
    %cmp/u;
    %jmp/1 T_0.9, 6;
    %jmp T_0.10;
T_0.2 ;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x55f4cd77eea0_0, 0, 8;
    %jmp T_0.10;
T_0.3 ;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x55f4cd77eea0_0, 0, 8;
    %jmp T_0.10;
T_0.4 ;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x55f4cd77eea0_0, 0, 8;
    %jmp T_0.10;
T_0.5 ;
    %pushi/vec4 8, 0, 8;
    %store/vec4 v0x55f4cd77eea0_0, 0, 8;
    %jmp T_0.10;
T_0.6 ;
    %pushi/vec4 16, 0, 8;
    %store/vec4 v0x55f4cd77eea0_0, 0, 8;
    %jmp T_0.10;
T_0.7 ;
    %pushi/vec4 32, 0, 8;
    %store/vec4 v0x55f4cd77eea0_0, 0, 8;
    %jmp T_0.10;
T_0.8 ;
    %pushi/vec4 64, 0, 8;
    %store/vec4 v0x55f4cd77eea0_0, 0, 8;
    %jmp T_0.10;
T_0.9 ;
    %pushi/vec4 128, 0, 8;
    %store/vec4 v0x55f4cd77eea0_0, 0, 8;
    %jmp T_0.10;
T_0.10 ;
    %pop/vec4 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55f4cd77eea0_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55f4cd734100;
T_1 ;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x55f4cd77f0a0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55f4cd77f260_0, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55f4cd77f360_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x55f4cd77f360_0;
    %cmpi/s 7, 0, 32;
    %jmp/0xz T_1.1, 5;
    %delay 10, 0;
    %load/vec4 v0x55f4cd77f360_0;
    %pad/s 3;
    %assign/vec4 v0x55f4cd77f0a0_0, 0;
    %pushi/vec4 5, 0, 32;
    %load/vec4 v0x55f4cd77f360_0;
    %cmp/s;
    %flag_or 5, 4;
    %jmp/0xz  T_1.2, 5;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55f4cd77f260_0, 0;
T_1.2 ;
    %delay 10, 0;
    %vpi_call 2 19 "$display", "Dec d_in=%0d => d_out=%0d on this i:%0d", v0x55f4cd77f0a0_0, v0x55f4cd77f190_0, v0x55f4cd77f360_0 {0 0 0};
    %load/vec4 v0x55f4cd77f360_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x55f4cd77f360_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
